{
  "name": "yupooscraper",
  "version": "1.0.0",
  "description": "Yupoo scraper with FastAPI backend and React frontend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "python -m uvicorn backend.main:app --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev -- --host 0.0.0.0",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  }
}
